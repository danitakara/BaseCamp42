- 

# EVEN LINES
ls -l | sed -e n\;d
ls -l | sed '0~2d'
ls -l | sed -n 'p;n'
ls -l | sed -n '1~2p'

ls -l | awk 'FNR%2'
ls -l | awk 'NR % 2 != 0'
ls -l | awk 'NR % 2 {print} !(NR % 2) && /pattern/ {print}'
ls -l | awk 'NR % 2 {print} !(NR % 2) {print | "grep pattern | rev" }'
ls -l | awk 'NR % 2 {print} !(NR % 2) {print length($0)}' 

ls -l | perl -ne 'print if $. % 2'

# ODD LINES
ls -l | sed -n 'n;p'
ls -l | awk 'NR % 2 == 0'
